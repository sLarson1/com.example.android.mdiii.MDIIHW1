
precision mediump float;

varying vec2 vTexPos;

uniform sampler2D uSampler;

void main(void) {
    gl_FragColor.a = texture2D(uSampler, vTexPos).a;
 //   gl_FragColor *= 0.1;
    gl_FragColor.r = vTexPos.x;
    gl_FragColor.g = vTexPos.y;
    gl_FragColor.b = 1.0;
   //  gl_FragColor.a *= 0.1;
//    gl_FragColor = texture2D(uSampler, vTexPos);

}